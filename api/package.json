{
  "name": "shopping-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#middlewares/*": "./middlewares/*",
    "#models/*": "./models/*",
    "#routes/*": "./routes/*",
    "#services/*": "./services/*",
    "#utils/*": "./utils/*"    
  },
  "scripts": {
    "prestart": "node ./swagger.js",
    "start": "cross-env NODE_ENV=local node -r dotenv/config ./bin/www-https.js",
    "dev": "cross-env NODE_ENV=development node -r dotenv/config ./bin/www.js",
    "local": "cross-env NODE_ENV=local node -r dotenv/config ./bin/www.js",
    "dbinit": "node -r dotenv/config ./samples/dbinit.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-validator": "^7.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^6.2.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "swagger-autogen": "^2.23.6",
    "swagger-ui-express": "^5.0.0",
    "tracer": "^1.3.0"
  }
}
